    \newcommand{\CDCFigSideways}[1]{\centerline{\includegraphics[width=7.8in,height=5.5in]{../Figures/#1}}}
    \DeclareGraphicsExtensions{.pdf}
    \ifthenelse{\boolean{bigPrint}}{\renewcommand{\titlepagefinish}{\newpage\large}}{}
\fi

