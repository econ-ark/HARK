    \makeatletter
    \renewenvironment{abstract}{%
       \begin{spacing}{0.9}
       \noindent {\tiny \phantom{.}} \\ % Trick to get proper spacing in html
       \noindent \hrule height 0.4pt depth 0.0pt width \textwidth \relax
       \vspace*{5mm}
       \noindent \textbf{Abstract}\\
       \indent \abstractSizeDefault
        }{%
           \noindent {\tiny \phantom{.}} \\  % Trick to get proper spacing in html
%           \noindent \hrule height 0.4pt depth 0.0pt width \textwidth \relax
