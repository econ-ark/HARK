  month =	 {apr},
  number =	 2,
  pages =	 {390--429},
  publisher =	 {The University of Chicago Press},
  title =	 {{A General Equilibrium Model of Housing, Taxes, and Portfolio Choice}},
  url =		 {https://www-journals-uchicago-edu.proxy.lib.ohio-state.edu/doi/abs/10.1086/261822 https://www.journals.uchicago.edu/doi/10.1086/261822},
